{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "0.1",
  "manifest_version": 2,
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src chrome://favicon;"
}